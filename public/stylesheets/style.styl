body

  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
a
  color: #000
  padding:8px;
  background:#ccc;
  margin-left:10px;

table {
	
	width: 100%;
	border-spacing: 0;
    border-collapse: collapse;
    text-align:center;
}

table tr th{
	
	background: #ccc;
	border:1px solid #000;
	height:30px;
}

table tr td{
	
	
	border:1px solid #000;
	height:30px;
}
table tr td img{
   width:100px;
}
.approvedVideo{
	width:100%;
	background-image:url("/image/pexels-photo.jpg");
	background-size: 100% 100%;
}
.approvedVideo tr td{
width:50%;
border: 0;
}

.approvedVideo tr td img{
width:100%;
padding:10px;
}
#pagination-demo{
  display: inline-block;
  margin-bottom: 1.75em;
}
#pagination-demo li{
  display: inline-block;
}
.approvedVideo-section{
